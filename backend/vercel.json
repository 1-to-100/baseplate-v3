{
  "version": 2,
  "buildCommand": "pnpm run vercel-build",
  "installCommand": "pnpm install",
  "routes": [
    {
      "src": "/api/swagger-ui-.*",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/favicon.*",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "git": {
    "deploymentEnabled": { "*": false, "vercel-deployment": true }
  }
}
