# Supabase Environment Variables
# Copy this file to .env and fill in your actual values

# Supabase Project Configuration
SUPABASE_PROJECT_REF=your-project-ref-here
SUPABASE_ACCESS_TOKEN=your-access-token-here

# Database Configuration
SUPABASE_DB_PASSWORD=your-database-password-here
SUPABASE_DB_URL=postgresql://postgres:[password]@db.[project-ref].supabase.co:5432/postgres

# API Configuration
# Required for bootstrap script and Edge Functions
SUPABASE_URL=https://[project-ref].supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Local Development (if using local Supabase)
SUPABASE_LOCAL_URL=http://localhost:54321
SUPABASE_LOCAL_ANON_KEY=your-local-anon-key-here

# Environment
NODE_ENV=development

# Bootstrap Configuration
# Disable automatic System Administrator creation on bootstrap
# Set to 'true' in production if you want to manually create admin users
DISABLE_BOOTSTRAP_ADMIN=false
